        cin>>alfabet;
        if(alfabet!= "\n"){
            //cout<<"g";
            jester_shild=false;
        }
        for(int i=0; i<x; i++){
            alfabet_simbols[i]=alfabet[i];
        }
        cout<<"number for folder";
        cin>>number_folder;
        int factorial=1;
        for(int i=0; i<x; i++){
            factorial=factorial*i;
        }
        factorial=factorial*255
        if(number_folder>0 && factorial<(255*255)){
            chek2=1;
        }
    }
    
    //int g=0;
    /*
